#include <iostream>
#include <cstring>

using namespace std;

struct BookType {
	int bookId;
	char title[256];
	char author[256];
	int year;
	float price;
};

int main() {

	BookType aBook;

	aBook.bookId = 1001;
	strcpy(aBook.title,"A Tale of Two Cities");

	return 0;
}
